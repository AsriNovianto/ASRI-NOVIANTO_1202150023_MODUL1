<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.studycase1.SecondActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="Makan Malam"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="27dp" />

    <TextView
        android:id="@+id/tempatMakan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:text="0"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="75dp" />
    //pada textView diatas menunjukkan Tempat makan yang diklik oleh button sebelumnya

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tempatMakan"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp"
        android:text="Menu"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="128dp" />

    <TextView
        android:id="@+id/menuMakan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tempatMakan"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="28dp"
        android:text="0"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="176dp" />
    //pada textView diatas menunjukkan menu makan yang diklik oleh button sebelumnya

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView4"
        android:layout_centerVertical="true"
        android:text="porsi"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="228dp" />


    <TextView
        android:id="@+id/porsiMakan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/menuMakan"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="14dp"
        android:text="0"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="276dp" />
    //pada textView diatas menunjukkan Prosi makan yang diklik oleh button sebelumnya

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView6"
        android:layout_below="@+id/porsiMakan"
        android:layout_marginTop="89dp"
        android:text="harga"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="343dp" />

    <TextView
        android:id="@+id/hargaTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"
        android:text="0"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="376dp" />
    //pada textView diatas menunjukkan Total Harga makan yang diklik oleh button sebelumnya
</RelativeLayout>
